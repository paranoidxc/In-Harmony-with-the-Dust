{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "framework"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Paranoid\\Framework\\Tests\\": "test/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "7.2.x-dev",
        "phpunit/phpunit": "10.5.x-dev"
    },
    "require": {
        "nikic/fast-route": "2.0.x-dev",
        "paranoid/fpm-framework": "dev-main"
    }
}
